{
  "name": "typegraphql-prisma2-auth-api-boilerplate",
  "version": "1.0.0",
  "description": "Authentication API boilerplate with TypeGraphQL, Prisma 2 and MySQL(docker container). Auto-generated GraphQL schema and resolvers",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run dev:start",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "mysql": "docker-compose exec db mysql -uroot -ptestdb",
    "generate": "prisma generate",
    "dev:start": "env-cmd -f .env.dev nodemon --exec ts-node --transpile-only ./src/index.ts",
    "prod:start": "env-cmd -f .env.prod nodemon --exec ts-node --transpile-only ./src/index.ts",
    "build": "tsc",
    "dev:migrate": "env-cmd -f .env.dev prisma migrate save --experimental",
    "dev:migrateup": "env-cmd -f .env.dev prisma migrate up --experimental",
    "prod:migrate": "env-cmd -f .env.prod prisma migrate save --experimental",
    "prod:migrateup": "env-cmd -f .env.prod prisma migrate up --experimental",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "keywords": [],
  "author": "Eunchurn Park(eunchurn.park@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "2.0.0-beta.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-shield": "^7.2.6",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "0.18.0-beta.16"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@typescript-eslint/typescript-estree": "^2.31.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.8.2",
    "typegraphql-prisma": "0.2.1",
    "typescript": "^3.8.3"
  }
}
